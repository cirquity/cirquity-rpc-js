{
  "name": "cirquity-rpc",
  "version": "1.0.1",
  "description": "Cirquity RPC API Interface in NodeJS",
  "main": "index.js",
  "files": [
    "dist/CirquityRPC.js",
    "lib/*"
  ],
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "test": "standard",
    "build-docs": "jsdoc -c jsdoc.json -t ./jsdoc/minami --verbose",
